{
  "name": "Auto complete dotProject",
  "description": "Complete tasks in dotProject easily",
  "version": "3.0",
  "manifest_version": 2,
  "icons": { "128": "./images/dotproject_128x128.png" },
  "background" : {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "permissions": ["idle",
    "tabs", "http://*/*/*"
  ],  
  "browser_action": {
    "default_title": "Create tasks",
    "default_icon": "./images/dotprj_16x16.png"
  }
}